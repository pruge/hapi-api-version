    "@hapi/hapi": ">=18.x.x"
  },
  "devDependencies": {
    "@hapi/hapi": "^18.3.1",
    "@hapi/code": "^5.3.1",
    "@hapi/lab": "^18.1.2"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.2",
    "@hapi/hoek": "^6.2.1",
    "@hapi/joi": "^15.0.2",
